import java.io.File;
import java.io.IOException;

public class OperacjaNaPliku {
	
	void UtworzPlik(String plik1){
		
		File stPlik1 = new File(plik1);
		try{
			if(!stPlik1.exists()){
				stPlik1.createNewFile();
			}		
		}
		catch(IOException e){
			System.out.println("B³¹d podczas tworzenia pliku. ");
		}
		
	}

}
